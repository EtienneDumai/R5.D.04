<odoo>
    <record id="view_form_vehicle" model="ir.ui.view">
        <field name="name">Garage Form</field>
        <field name="model">rentcars.garage</field>
        <field name="arch" type="xml">
            <form string="garage">
                <group>
                    <field name="active"/>
                    <field name="address_street"/>
                    <field name="address_zipcode"/>
                    <field name="address_city"/>
                    <field name="places_max"/>
                    <field name="vehicle_ids" widget="many2many">
                        <list editable="bottom">
                            <field name="model"/>
                            <field name="immatriculation" />
                            <field name="date_purchased" />
                            <field name="thumbnail" widget="image" options='{"size": [150, 140]}'/>
                        </list>
                    </field>
                </group>
            </form>
        </field>
    </record>
</odoo>
