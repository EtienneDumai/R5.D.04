<odoo>
    <record id="view_form_vehicle" model="ir.ui.views">
        <field name="name">Vehicle Form</field>
        <field name="model">rentcars.vehicle</field>
        <field name="arch" type="xml">
            <form string="vehicle">
                <header>
                    <button name="button_check_immatriculation" string="Check immatriculation" type="object"/>
                </header>
                <group>
                    <field name="model"/>
                    <field name="thumbnail" widget="image" options='{"size": [150, 140]}'/>
                    <field name="active"/>
                    <field name="date_purchased"/>
                    <field name="immatriculation"/>
                </group>
            </form>
        </field>
    </record>
</odoo>